<deployedServers>
    <global
        multicastHost="%heartbeat_ip%"
        multicastPort="%heartbeat_port%"
    />
    <featureServer
        host="%host%"
        port="%fsvm_port%"
        type="Voicemail"
        uaVMHost="%host%"
        uaVMFirstPort="%uavm_port1%"
        uaVMLastPort="%uavm_port5%"
    />
		
    <heartbeatServer
        host="%host%"
    />
    <marshalServer
        host="%host%"
        port="%ms_ua_port%"
        type="UserAgent"
        gatewayHost="%ms_ua_gwhost%"
        gatewayPort="%ms_ua_gwport%"
    />
    <marshalServer
        host="%host%"
        port="%ms_gw_port%"
        type="Gateway"
        gatewayHost="%ms_gw_gwhost%"
        gatewayPort="%ms_gw_gwport%"
    />
    <marshalServer
        host="%host%"
        port="%ms_inet_port%"
        type="Internetwork"
    />
    <featureServer
        host="%host%"
        port="%fs_cb_port%"
        type="CallBlocking"
    />
    <featureServer
        host="%host%"
        port="%fs_fna_port%"
        type="ForwardNoAnswerBusy"
    />
    <featureServer
        host="%host%"
        port="%fs_cs_port%"
        type="CallScreening"
    />
    <featureServer
        host="%host%"
        port="%fs_cfwa_port%"
        type="ForwardAllCalls"
    />
    <featureServer
        host="%host%"
        port="%fs_cr_port%"
        type="CallReturn"
    />
    <featureServer
        host="%host%"
        port="%fs_clidx_port%"
        type="CallerIdBlocking"
    />
    <redirectServer
        host="%host%"
        port="%rs_port%"
        syncPort="22002"
    />
</deployedServers>
