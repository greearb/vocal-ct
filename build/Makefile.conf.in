
# this is a simple settings file

VOCAL_MAKE=@GNU_MAKE@

ARCH=@VOCAL_ARCH@

VOCAL_HAVE_DO_NOT_DISTRIBUTE=@HAVE_DO_NOT_DISTRIBUTE@

VOCAL_USE_TOOLCHAIN=@VOCAL_USE_TOOLCHAIN@
ifneq ($(VOCAL_USE_TOOLCHAIN),default)
    VOCAL_TOOLCHAIN_TYPE := $(VOCAL_USE_TOOLCHAIN)
endif

VOCAL_WITH_CROSS_ROOT=@VOCAL_CROSS_ROOT@
ifneq ($(VOCAL_WITH_CROSS_ROOT),default)
    VOCAL_CROSS_ROOT = $(VOCAL_WITH_CROSS_ROOT)
endif

USE_REPO=@VOCAL_USE_GCC_REPO@

VOCAL_BUILD_SHARED_LIBS=@VOCAL_BUILD_SHARED_LIBS@

VOCAL_USE_SYSTEM_LIBS=@VOCAL_USE_SYSTEM_LIBS@
VOCAL_USE_SYSTEM_LIBSNDFILE=@VOCAL_USE_SYSTEM_LIBSNDFILE@
VOCAL_SNDFILE_LIB_DIR=@VOCAL_SNDFILE_LIB_DIR@
VOCAL_SNDFILE_INC_DIR=@VOCAL_SNDFILE_INC_DIR@

VOCAL_BUILD_GPROF=@VOCAL_BUILD_GPROF@

VOCAL_USE_LANFORGE=@VOCAL_USE_LANFORGE@

VOCAL_USE_VOICEAGE=@VOCAL_USE_VOICEAGE@
VOCAL_USE_SPEEX=@VOCAL_USE_SPEEX@

VOCAL_COMPILE_TYPE=@VOCAL_COMPILE_TYPE@
CODE_OPTIMIZE=@VOCAL_OPTIMIZE_LEVEL@

# Have to configure this by hand if you want older libsndfile.
#if using new send file
SNDFILEVERSION=libsndfile-1.0.5
DEFINES += NEW_SNDFILE
#else
# Un-comment these if you want to use the older sendfile version.
#SNDFILEVERSION=libsndfile-0.0.24
#endif



######################################################################
# openssl stuff
######################################################################

VOCAL_USE_OPENSSL=@VOCAL_USE_OPENSSL@
VOCAL_OPENSSL_PKGCONFIG=@VOCAL_OPENSSL_PKGCONFIG@
VOCAL_OPENSSL_INCLUDES=@VOCAL_OPENSSL_INCLUDES@
VOCAL_OPENSSL_LDLIBS=@VOCAL_OPENSSL_LDLIBS@


VOCAL_AR_PATH=@VOCAL_AR_PATH@

VOCAL_SOCKLEN_TYPE=@VOCAL_SOCKLEN_TYPE@
ifneq ($(VOCAL_SOCKLEN_TYPE),socklen_t)
    VOCAL_USE_SOCKLEN_T := $(VOCAL_SOCKLEN_TYPE)
endif

VOCAL_SOCKET_BUFFER_TYPE=@VOCAL_SOCKET_BUFFER_TYPE@
ifneq ($(VOCAL_SOCKET_BUFFER_TYPE),void)
    VOCAL_USE_SOCKET_BUFFER := $(VOCAL_SOCKET_BUFFER_TYPE)
endif

VOCAL_SPHINX_PATH=@VOCAL_SPHINX_PATH@
ifneq ($(VOCAL_SPHINX_PATH),default)
    VOCAL_USE_SPHINX := true
endif

VOCAL_USE_NEWPROV=@VOCAL_USE_NEWPROV@

VOCAL_USE_ENUM=@VOCAL_USE_ENUM@

VOCAL_HAS_POSTGRESQL=@VOCAL_HAS_POSTGRESQL@

INSTALL_PATH=@prefix@
