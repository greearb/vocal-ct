#AC_LANG(C++)




echo $ac_n checking for "do-not-distribute" directory... "" ; 
if test -d "do-not-distribute" ; then HAVE_DO_NOT_DISTRIBUTE=1 ; echo 'found' ; else HAVE_DO_NOT_DISTRIBUTE=0 ; echo 'not found' ; fi
 # Extract the first word of "make", so it can be a program name with args.
set dummy make; ac_word=$2
echo $ac_n "checking for $ac_word""... $ac_c" 1>&6
echo "configure:579: checking for $ac_word" >&5
if eval "test \"`echo '$''{'ac_cv_path_GNU_MAKE'+set}'`\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  case "$GNU_MAKE" in
  /*)
  ac_cv_path_GNU_MAKE="$GNU_MAKE" # Let the user override the test with a path.
  ;;
  ?:/*)			 
  ac_cv_path_GNU_MAKE="$GNU_MAKE" # Let the user override the test with a dos path.
  ;;
  *)
  IFS="${IFS= 	}"; ac_save_ifs="$IFS"; IFS=":"
  ac_dummy="$PATH:/usr/bin:/usr/local/bin"
  for ac_dir in $ac_dummy; do 
    test -z "$ac_dir" && ac_dir=.
    if test -f $ac_dir/$ac_word; then
      ac_cv_path_GNU_MAKE="$ac_dir/$ac_word"
      break
    fi
  done
  IFS="$ac_save_ifs"
  test -z "$ac_cv_path_GNU_MAKE" && ac_cv_path_GNU_MAKE="''"
  ;;
esac
fi
GNU_MAKE="$ac_cv_path_GNU_MAKE"

